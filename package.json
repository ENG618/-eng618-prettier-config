{
  "name": "@eng618/prettier-config",
  "version": "1.5.0",
  "description": "My personal Prettier config",
  "main": "index.json",
  "repository": "git@github.com:eng618/prettier-config.git",
  "author": "Eric N. Garcia <eng618@garciaericn.com>",
  "license": "MIT",
  "scripts": {
    "release": "auto shipit"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "^10.13.2",
    "@auto-it/conventional-commits": "^10.13.2",
    "@auto-it/twitter": "^10.13.2",
    "auto": "^10.13.2"
  },
  "auto": {
    "plugins": [
      [
        "twitter",
        {
          "message": "v%version of %package was released!\n\n%link",
          "threshold": "patch"
        }
      ],
      "npm",
      "all-contributors",
      "conventional-commits",
      "released"
    ]
  }
}
